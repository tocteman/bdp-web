{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "1.1.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "@fortawesome/react-fontawesome": "0.0.19",
    "gatsby": "^1.9.256",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-source-filesystem": "^1.5.35",
    "gatsby-transformer-remark": "^1.7.40",
    "react-granim-canvas": "^1.0.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "typeface-clear-sans": "0.0.44",
    "typeface-ibm-plex-sans": "0.0.58"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tailwind build ./src/layouts/index.tailwind.css -c ./tailwind.js -o ./src/layouts/index.css && purgecss -c purgecss.config.js -o ./src/layouts && gatsby build",
    "develop": "tailwind build ./src/layouts/index.tailwind.css -c ./tailwind.js -o ./src/layouts/index.css && gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.11.1",
    "purgecss": "^0.21.0",
    "tailwindcss": "^0.5.1"
  }
}
